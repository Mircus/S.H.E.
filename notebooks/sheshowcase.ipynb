    "def create_multilayer_network():\n",
    "    \"\"\"Create a multilayer network with different types of relationships\"\"\"\n",
    "    \n",
    "    people = ['Alice', 'Bob', 'Charlie', 'Diana', 'Eve', 'Frank']\n",
    "    \n",
    "    # Layer 1: Professional relationships\n",
    "    professional = nx.Graph()\n",
    "    professional.add_nodes_from(people)\n",
    "    prof_edges = [('Alice', 'Bob'), ('Bob', 'Charlie'), ('Alice', 'Charlie'),\n",
    "                  ('Diana', 'Eve'), ('Eve', 'Frank')]\n",
    "    professional.add_edges_from(prof_edges)\n",
    "    \n",
    "    # Layer 2: Social relationships  \n",
    "    social = nx.Graph()\n",
    "    social.add_nodes_from(people)\n",
    "    social_edges = [('Alice', 'Diana'), ('Bob', 'Eve'), ('Charlie', 'Frank'),\n",
    "                    ('Alice', 'Eve'), ('Diana', 'Frank')]\n",
    "    social.add_edges_from(social_edges)\n",
    "    \n",
    "    # Layer 3: Collaboration (higher-order)\n",
    "    collaborations = [\n",
    "        ['Alice', 'Bob', 'Diana'],    # Project A\n",
    "        ['Charlie', 'Eve', 'Frank'],  # Project B\n",
    "        ['Alice', 'Charlie', 'Eve']   # Project C\n",
    "    ]\n",
    "    \n",
    "    return {'professional': professional, 'social': social}, collaborations\n",
    "\n",
    "print(\"üåê Multilayer Simplicial Network Analysis:\")\n",
    "print(\"=\" * 43)\n",
    "\n",
    "layers, collaborations = create_multilayer_network()\n",
    "\n",
    "# Analyze each layer\n",
    "layer_stats = {}\n",
    "for layer_name, G in layers.items():\n",
    "    try:\n",
    "        layer_complex = SHEDataLoader.from_networkx(G, include_cliques=True)\n",
