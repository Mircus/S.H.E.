    "def simulate_temporal_network():\n",
    "    \"\"\"Simulate a network that evolves over time\"\"\"\n",
    "    \n",
    "    # Base network\n",
    "    nodes = list(range(10))\n",
    "    timesteps = 5\n",
    "    \n",
    "    temporal_snapshots = []\n",
    "    \n",
    "    for t in range(timesteps):\n",
    "        G = nx.Graph()\n",
    "        G.add_nodes_from(nodes)\n",
    "        \n",
    "        # Evolving connectivity\n",
    "        for i in nodes:\n",
    "            for j in nodes[i+1:]:\n",
    "                # Probability of connection changes over time\n",
    "                prob = 0.3 + 0.1 * np.sin(2 * np.pi * t / timesteps + i + j)\n",
    "                if np.random.random() < prob:\n",
    "                    G.add_edge(i, j)\n",
    "        \n",
    "        temporal_snapshots.append(G)\n",
    "    \n",
    "    return temporal_snapshots\n",
    "\n",
    "print(\"‚è∞ Temporal Simplicial Complex Analysis:\")\n",
    "print(\"=\" * 42)\n",
    "\n",
    "# Generate temporal network\n",
    "snapshots = simulate_temporal_network()\n",
    "temporal_stats = []\n",
    "\n",
    "for t, G in enumerate(snapshots):\n",
    "    # Convert to simplicial complex and analyze\n",
    "    try:\n",
    "        temp_complex = SHEDataLoader.from_networkx(G, include_cliques=True)\n",
    "        triangles = list(temp_complex.complex.skeleton(2))\n",
    "        \n",
    "        stats = {\n",
    "            'timestep': t,\n",
    "            'nodes': G.number_of_nodes(),\n",
    "            'edges': G.number_of_edges(),\n",
    "            'triangles': len(triangles),\n",
    "            'clustering': nx.average_clustering(G)\n",
    "        }\n",
    "        temporal_stats.append(stats)\n",
    "        \n",
    "    except Exception as e:\n",
    "        # Fallback for when SHE is not fully available\n",
